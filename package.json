{
  "name": "cognicreative-website",
  "version": "1.0.0",
  "description": "CogniCreative website",
  "main": "server.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "dev": "webpack-dev-server --hot --inline --devtool inline-source-map",
    "lint": "eslint src/**",
    "lint:watch": "esw -w src/**",
    "deploy": "git push heroku master",
    "build-start": "npm run build && npm run start-prod",
    "start": "npm run start-prod",
    "start-prod": "better-npm-run start-prod",
    "test": "mocha --compilers js:babel-core/register ./test/**/*.spec.js --require ignore-styles"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:cognicreative/cognicreative-website.git"
  },
  "betterScripts": {
    "start-prod": {
      "command": "node server.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.4",
    "enzyme": "^2.8.2",
    "expect": "^1.20.2",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "fullpage.js": "^2.9.4",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "jquery": "^3.2.1",
    "mocha": "^3.4.2",
    "node-sass": "^4.5.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.5.4",
    "react-motion": "^0.5.0",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-youtube": "^7.4.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "webpack": "^2.6.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "eslint-watch": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sass-lint": "^1.10.2",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^2.4.5",
    "webpack-hot-middleware": "^2.18.0"
  }
}
